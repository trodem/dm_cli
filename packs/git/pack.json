{
  "description": "Git workflows, aliases, and troubleshooting notes",
  "summary": "Git aliases and project actions",
  "tags": [
    "git",
    "vcs"
  ],
  "examples": [
    "dm -p git find rebase",
    "dm -p git run gs",
    "dm git-tools graph"
  ],
  "jump": {},
  "run": {
    "gs": "git status",
    "gss": "git status -s",
    "gl": "git log --oneline --graph --decorate",
    "gpl": "git pull",
    "gps": "git push",
    "gco": "git checkout",
    "gbr": "git branch",
    "gcl": "git clone",
    "gst": "git stash",
    "gsta": "git stash apply",
    "gcommit": "git add . && git commit"
  },
  "projects": {
    "git-tools": {
      "path": "projects/git-tools",
      "commands": {
        "status": "git status",
        "sync": "git fetch --all --prune",
        "prune-merged": "for /f \"tokens=*\" %i in ('git branch --merged ^| findstr /v /c:\"*\" /c:\"main\" /c:\"master\"') do git branch -d %i",
        "graph": "git log --oneline --graph --decorate --all",
        "last-10": "git log -10 --oneline",
        "authors": "git shortlog -sn",
        "changelog": "git log --pretty=format:\"%h %ad %s\" --date=short",
        "tags": "git tag -n"
      }
    }
  },
  "search": {
    "knowledge": "packs/git/knowledge"
  }
}
